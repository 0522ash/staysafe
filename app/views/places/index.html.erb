<main class="main">
  <h1>観光情報</h1>
  <div class="inner">
    <% if user_signed_in? %>
      <div class="share">
        <%= link_to "情報をシェアする", new_place_path, class: :nav_btn %>
      </div>
    <% end %>
    <div class="card_wrapper">
      <%= render partial: 'place', collection: @places %>
    </div>
  </div>
</main>
